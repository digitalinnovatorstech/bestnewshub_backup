{
  "name": "blog-website",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon index.js",
    "dev": "concurrently \"nodemon index.js\"",
    "db:create": "sequelize db:create --charset utf8mb4 --collate utf8mb4_unicode_ci",
    "db:migrate": "sequelize db:migrate",
    "prod:db:migrate": "cross-env NODE_ENV=production sequelize db:migrate",
    "db:drop": "sequelize db:drop",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "undo-migration": "cross-env NODE_ENV=production sequelize db:migrate:undo --name",
    "prod:db:create": "cross-env NODE_ENV=production sequelize db:create --charset utf8mb4 --collate utf8mb4_unicode_ci",
    "db:seed": "sequelize db:seed:all",
    "prod:db:seed": "NODE_ENV=production sequelize db:seed:all",
    "prod:db:drop": "NODE_ENV=production sequelize db:drop --charset utf8mb4 --collate utf8mb4_unicode_ci",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@stripe/stripe-js": "^3.2.0",
    "api": "^6.1.1",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "canvas": "^2.11.2",
    "convert-excel-to-json": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-ip": "^1.0.4",
    "firebase-admin": "^12.6.0",
    "flatted": "^3.2.7",
    "fs": "^0.0.1-security",
    "google-auth-library": "^9.8.0",
    "handlebars": "^4.7.8",
    "html-to-image": "^1.11.11",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "msg91": "^0.0.7",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.9.5",
    "next": "^15.1.7",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
